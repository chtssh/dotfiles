def_key "k"
  scroll_up

def_key "shift-k"
  select_item
  scroll_up

def_key "j"
  scroll_down

def_key "shift-j"
  select_item
  scroll_down

def_key "ctrl-u"
  page_up

def_key "ctrl-d"
  page_down

def_key "g"
  move_home

def_key "G"
  move_end

def_key "d"
  delete_playlist_items

def_key "l"
  next_column

def_key "h"
  previous_column

def_key "l"
  show_lyrics

def_key "K"
  move_selected_items_up

def_key "J"
  move_selected_items_down
